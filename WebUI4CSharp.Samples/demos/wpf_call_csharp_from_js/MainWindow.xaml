<Window x:Name="MainForm" x:Class="wpf_call_csharp_from_js.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:wpf_call_csharp_from_js"
        mc:Ignorable="d"
        Title="call_csharp_from_js" Height="300" Width="400" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closing="MainForm_Closing">
    <DockPanel x:Name="PnlMain">
        <Button x:Name="ShowBrowserBtn" Content="Show browser" Width="NaN" DockPanel.Dock="Top" Height="21" Margin="5,5,5,5" VerticalAlignment="Top" Click="ShowBrowserBtn_Click"/>
        <TextBox x:Name="Memo1" TextWrapping="Wrap" Margin="5,0,5,5" VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto"/>
    </DockPanel>
</Window>
